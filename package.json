{
  "name": "sandbox-express",
  "version": "0.0.1",
  "description": "Sandbox project for playing with Express",
  "main": "index.client.js",
  "scripts": {
    "lint": "npx eslint ./** --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint ./** --ext .js,.jsx,.ts,.tsx --fix",
    "ts:check": "npx tsc --noEmit",
    "typeorm:cli": "cross-env NODE_ENV=migration npx ts-node -r dotenv-defaults/config -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:cli:ds": "npm run typeorm:cli -- -d ./src/server/orm/datasource.ts",
    "migr:gen": "npm run typeorm:cli:ds migration:generate",
    "migr:gen:initial": "npm run migr:gen ./src/server/orm/migrations/initial",
    "migr:create": "npm run typeorm:cli migration:create",
    "migr:run": "npm run typeorm:cli:ds migration:run",
    "migr:revert": "npm run typeorm:cli:ds migration:revert",
    "migr:show": "npm run typeorm:cli:ds migration:show",
    "schema:log": "npm run typeorm:cli:ds schema:log",
    "schema:drop": "npm run typeorm:cli:ds schema:drop",
    "dev:client": "cross-env NODE_ENV=development npx webpack serve --config=configs/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development npx webpack --config=configs/webpack.config.server.js -w",
    "dev": "npm run dev:client",
    "build:client": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production npx webpack --config=configs/webpack.config.server.js",
    "analyze:client": "cross-env BUNDLE_ANALYZER=true npm run build:client",
    "analyze:server": "cross-env BUNDLE_ANALYZER=true npm run build:server",
    "start": "node dist/server.js",
    "prod": "npm run build:client && npm run build:server && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nk11dev/sandbox-express.git"
  },
  "author": "nk11dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nk11dev/sandbox-express/issues"
  },
  "homepage": "https://github.com/nk11dev/sandbox-express#readme",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@types/express": "4.17.17",
    "@types/cookie-parser": "1.4.3",
    "@types/morgan": "1.9.4",
    "@types/node": "20.3.1",
    "@types/pg": "8.10.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/redux-logger": "3.0.9",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "8.44.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "nodemon-webpack-plugin": "4.8.1",
    "react-refresh": "0.14.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "typescript-plugin-css-modules": "5.0.1",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-dev-server": "4.15.1"
  },
